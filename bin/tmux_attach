#!/bin/env bash

sessions=$(tmux ls 2>/dev/null | grep -o '^[^:]*')

if [[ -n "$sessions" ]]; then
    selected_session=$(echo "$sessions" | fzf-tmux -h +s --prompt="Session: " --print-query | tail -1)

    if [[ -n "$selected_session" ]]; then
        ~/Dots/bin/t $selected_session
    fi
else
    ~/Dots/bin/t
fi
